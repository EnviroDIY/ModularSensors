            "authors": [
                "Mike 'GreyGnome' Schwager"
            ],
            "frameworks": "arduino",
            "platforms": "atmelavr, atmelsam"
        },
        {
            "name": "SdFat",
            "library id": "322",
            "url": "https://github.com/greiman/SdFat.git",
            "version": "=1.1.0",
            "note": "SdFat - FAT16/FAT32 file system for SD cards.",
